{
  "name": "transport-aggregator",
  "version": "1.0.0",
  "description": "Transport data aggregator with gamified learning branches",
  "type": "module",
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "test": "jest --runInBand",
    "coverage": "jest --coverage --runInBand",
    "next": "node .game/unlock.mjs",
    "hint": "node .game/hint.mjs",
    "reset:here": "git restore -s HEAD -- .",
    "respawn": "git restore -s origin/$(git branch --show-current) -- ."
  },
  "keywords": ["transport", "typescript", "learning", "async"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}